{
    "name": "util-tools",
    "version": "0.0.1",
    "main": "./lib/index.js",
    "module": "es/index.js",
    "unpkg": "dist/util-tools.js",
    "types": "./es/index.d.ts",
    "files": [
        "dist",
        "lib",
        "src",
        "es"
    ],
    "scripts": {
        "build:cjs": "cross-env BABEL_ENV=commonjs babel src --extensions \".js,.ts,.tsx,.jsx\" --out-dir lib",
        "build:es": "babel src --extensions \".js,.ts,.tsx,.jsx\" --out-dir es",
        "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/util-tools.js",
        "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/util-tools.min.js",
        "build:types": "tsc",
        "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:umd && yarn build:umd:min",
        "clean": "rimraf lib dist es",
        "prepare": "yarn clean && yarn build"
    },
    "dependencies": {
        "@babel/runtime": "^7.20.1",
        "axios": "^1.1.3",
        "classname": "^0.0.0",
        "lodash.clonedeep": "^4.5.0",
        "mobx": "^6.6.2",
        "mobx-react-lite": "^3.4.0",
        "mockjs": "^1.1.0",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.2",
        "@babel/plugin-proposal-decorators": "^7.20.2",
        "@babel/plugin-transform-react-jsx": "^7.19.0",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@rollup/plugin-babel": "^6.0.2",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-json": "^5.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.1",
        "@types/axios": "^0.14.0",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/node": "^18.11.7",
        "@types/react": "^18.0.25",
        "cross-env": "^7.0.3",
        "glob": "^8.0.3",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.1",
        "rollup-plugin-terser": "^7.0.2"
    }
}